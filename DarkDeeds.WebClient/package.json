{
  "name": "dark-deeds",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aspnet/signalr": "^1.1.4",
    "connected-react-router": "^4.5.0",
    "inversify": "^5.0.1",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-dragula": "^1.1.17",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0",
    "react-toastify": "^4.5.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.83.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "build-js": "react-scripts-ts build",
    "build-copy": "npx copyfiles \"build/**\" \"../DarkDeeds/DarkDeeds.Api/wwwroot\" -u 1 -e build && echo files copied",
    "build": "npm run build-css && npm run build-js && npm run build-copy",
    "test-ci": "npm run build-css && cross-env CI=true react-scripts-ts test --env=jsdom",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "cross-env BROWSER=none react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.14.18",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^6.0.14",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.5",
    "copyfiles": "2.1.0",
    "cross-env": "^5.2.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "react-test-renderer": "^16.9.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.3"
  }
}
