{
  "name": "dark-deeds",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "fmt": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|scss|md|ts|tsx|css)\"",
    "fmt:check": "prettier --check \"**/*.+(js|jsx|json|yml|yaml|scss|md|ts|tsx|css)\"",
    "test": "vitest",
    "test:run": "vitest run",
    "ci": "npm run fmt:check && npm run lint && npm run build && npm run test:run",
    "mcp-browser-tools": "npx @agentdeskai/browser-tools-server@1.2.0"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.7",
    "@reduxjs/toolkit": "^2.2.8",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/redux-logger": "^3.0.13",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "prettier": "3.3.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.19",
    "vitest": "^2.1.4"
  }
}
